{
  "name": "study-rankings",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "npx cross-env NODE_ENV=test mocha --exit --recursive --timeout 10000",
    "prettier": "npx prettier --write .",
    "lint": "npx prettier --write . && npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "db-init": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all --seeders-path seeders/constants",
    "fake-seeds": "npx sequelize db:seed:all --seeders-path seeders/fakes",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npx prettier --write",
      "npx eslint ."
    ]
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/sequelize": "^4.1.1",
    "adminjs": "^7.8.13",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "formidable": "^3.5.2",
    "glob": "^11.0.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "morgan": "~1.9.1",
    "mysql2": "^3.11.5",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.3.0",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "mocha": "^11.0.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0"
  }
}
